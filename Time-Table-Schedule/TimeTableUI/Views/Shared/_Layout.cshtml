                    <!--begin::Container-->
                    <div class = "container">
                        <!--begin::Dashboard-->
                        @RenderBody()
                        <!--end::Dashboard-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Entry-->
            </div>
            <!--end::Content-->
            <!--begin::Footer-->

            @*@await Html.PartialAsync("_Footer")*@
